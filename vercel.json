{
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "projects": [
    {
      "path": "admin-panel",
      "name": "b2b-v1-admin"
    },
    {
      "path": "seller-panel",
      "name": "b2b-v1-seller"
    },
    {
      "path": "store-front",
      "name": "b2b-v1-storefront"
    }
  ],
  "build": {
    "env": {
      "DEPLOY_ADMIN": "true",
      "DEPLOY_SELLER": "true",
      "DEPLOY_STORE": "true"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": true
    }
  }
}